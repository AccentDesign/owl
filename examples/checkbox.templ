package examples

import (
    "github.com/accentdesign/owl/components"
    "github.com/accentdesign/owl/utils"
)

templ Checkbox() {
    @Base() {
        @utils.InlineCss("css/checkbox.css")

        <h2 class="owl-h2 mb-10">checkbox</h2>

        <label class="flex items-center gap-2">
            @components.Checkbox()
            <span>Checkbox</span>
        </label>
    }
}