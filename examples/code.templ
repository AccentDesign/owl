package examples

var codeStyles = []string{"typography.css", "code.css"}

templ Code() {
	@Base(codeStyles) {
		<h1 class="owl-h1 mb-10">code</h1>
		<p class="mb-6">
			Some inline
			<code class="owl-code mx-1">code</code>
			block.
		</p>
		<pre class="owl-pre">
			<code class="owl-code">
				git clone https://github.com/AccentDesign/owl.git
			</code>
		</pre>
	}
}
